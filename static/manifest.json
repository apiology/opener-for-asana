{
  "name": "Opener for Asana",
  "description": "Searches for and opens an Asana task in the Chrome Omnibox",
  "version": "0.1.1",
  "icons": {
    "16": "apiology-16x16.png",
    "48": "apiology-48x48.png",
    "128": "apiology-128x128.png"
  },
  "permissions": [
    "storage"
  ],
  "omnibox": { "keyword": "a" },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "manifest_version": 2
}
