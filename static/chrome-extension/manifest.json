{
  "name": "Opener for Asana",
  "description": "Searches for and opens an Asana task in the Chrome Omnibox or as an Alfred workflow",
  "version": "0.2.0",
  "icons": {
    "16": "apiology-16x16.png",
    "48": "apiology-48x48.png",
    "128": "apiology-128x128.png"
  },
  "permissions": [
    "storage"
  ],
  "omnibox": { "keyword": "a" },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "manifest_version": 3
}
