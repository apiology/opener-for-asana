{
  "name": "Opener for Asana",
  "description": "Searches for and opens an Asana task in the Chrome Omnibox",
  "version": "0.1.0",
  "permissions": [
    "activeTab"
  ],
  "omnibox": { "keyword": "a" },
  "background": {
    "scripts": ["dist/background.js"],
    "persistent": false
  },
  "manifest_version": 2
}
